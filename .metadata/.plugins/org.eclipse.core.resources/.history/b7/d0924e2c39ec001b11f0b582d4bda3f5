
public class InterfaceCheckers implements DefaultInterface{

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		InterfaceCheckers checker = new InterfaceCheckers();
        checker.run();
        checker.sleep();

	}

	@Override
	public void sleep() {
		// TODO Auto-generated method stub
		System.out.println("Sleeping...");
		
	}

}
